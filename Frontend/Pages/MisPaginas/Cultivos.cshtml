@page
@model Frontend.Pages.CultivosModel
@{
}
<h>INGRESAR DATOS DEL CULTIVO</h>

<table class="table">
    @foreach (var Cultivos in Model.Cultivos)
    {
        <tr>
            <td>@Cultivos.cargo </td>
            <td>@Cultivos.salario </td>
            <td>@Cultivos.codigoCarga </td>
           
            <td>
                <a class = "btn btn-primary table-btn" asp-page="./Details" asp-route-CultivosId="@Cultivos.Id">
                    <i class="fas fa-info-circle fa-lg" ></i>
                </a>                
            </td>
            <td>
                <a class="btn btn-primary table-btn" asp-page="./Edit" asp-route-Cultivos="@Cultivos.Id">
                <i class="fas fa-address-book fa-lg" aria-hidden="true"></i>
                </a>
              
            </td>
        </tr>
        
    }





</table>
<a asp-page="./Edit" class="btn btn-primary">Adicionar INFORMACION DEL Cultivo </a>

<div class = "btn-item">  
       <center>  <a href="/MisPaginas/Registro" class="btn-ghost round"> Crear Usuario</a></center>
     </div>